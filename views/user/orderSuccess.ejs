        <style>
            @keyframes bounce {
  0%, 20%, 50%, 80%, 100% {
    transform: translateY(0);
  }
  40% {
    transform: translateY(-30px);
  }
  60% {
    transform: translateY(-15px);
  }
}

.animated-bounce {
  animation: bounce 2s;
}

            .order-success-page {
      margin-top: 50px;
      padding: 30px;
      background-color: #f9f9f9;
      border-radius: 8px;
    }
    
    .success-icon {
      font-size: 100px;
      color: #28a745;
      margin-bottom: 20px;
    }
    
    .order-summary {
      padding: 20px;
      background-color: #fff;
      border: 1px solid #ddd;
      border-radius: 8px;
      text-align: left;
      margin-top: 20px;
    }
    
    .order-summary p {
      font-size: 18px;
    }
    
    .order-actions a {
      margin: 5px;
      min-width: 160px;
    }
    
    .support-info {
      margin-top: 30px;
    }
    
    .support-info a {
      color: #007bff;
      text-decoration: underline;
    }
    
        </style>
    
